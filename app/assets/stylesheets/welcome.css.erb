  float: left;
}

/* Undo the collapsing navbar */
.primary-nav .navbar-collapse {
  display: block !important;
  height: auto !important;
  padding-bottom: 0;
  overflow: visible !important;
}

.primary-nav .navbar-toggle {
  display: none;
}
