.large-overlay {
	overflow-y: scroll;
	position: fixed;
	width: 320px;
	height:100%;
	z-index:10000;
	padding: 50px 40px;
	background-color: rgba(10,10,10,0.95);
}

.large-overlay h1.nav-hero {
	font-weight: 100;
	font-size: 28px;
	color: #fff;
	margin-bottom: 30px;
	position: relative;
}

.large-overlay h1.nav-hero span.glyphicon {
	float:right;
	color: #aaa;
}

.large-overlay h1.nav-hero span.glyphicon:hover {
	color: #fff;
	cursor: pointer;
	}

nav.menu-overlay {
	right:-320px;
	transition: all 0.3s ease;
}

nav.show-right {
	right:0;
	transition: all 0.3s ease;
}


nav.menu-overlay ul.main {
	list-style: none;
	padding: 0px;
	width:200px;
	position:relative;
}

nav.menu-overlay ul.main>li {
	text-transform: uppercase;
	letter-spacing: 2px;
	color: #fff;
	margin-bottom: 10px;
	font-size: 14px;
	font-weight: 600;
}

nav.menu-overlay ul.sub a {
	color:#ccc;
}

nav.menu-overlay ul.sub a:hover {
	color:#ff8500;
}

nav.menu-overlay ul.sub {
	-webkit-margin-collapse: separate;
	list-style:none;
	padding:0px;
	color:#ccc;
	padding-left: 15px;
	border-left: 3px solid #ccc;
	margin: 3px 10px 10px;
	text-transform: uppercase;
	letter-spacing: 2px;
	font-size: 14px;
	font-weight: 300;
}

nav.menu-overlay ul.sub>li {
	margin: 7px 0px;
}


nav.menu-overlay ul a.navigation:hover:before {
	font-family: 'Glyphicons Halflings';
	-webkit-font-smoothing: antialiased;
	content: "\e131";
	font-size: 14px;
	position:absolute;
	animation: menuicon 0.5s;
	-webkit-animation: menuicon 0.5s; /* Safari and Chrome */
	right: 10px;
}

@keyframes menuicon
{
from {right: 0px; opacity: 0}
to {right: 10px; opacity: 1}
}

@-webkit-keyframes menuicon /* Safari and Chrome */
{
from {right: 0px; opacity: 0}
to {right: 10px; opacity: 1}
}

nav.menu-overlay ul a.toggle:hover:before {
	font-family: 'Glyphicons Halflings';
	-webkit-font-smoothing: antialiased;
	content: "\e114";
	font-size: 14px;
	position:absolute;
	animation: menuicon 0.5s;
	-webkit-animation: menuicon 0.5s; /* Safari and Chrome */
	right: 10px;
}

nav.menu-overlay ul>li>a>.glyphicon {
	font-size: 11px;
	margin-right: 7px;
}



.moveleft {
	-webkit-filter: grayscale(0.7) blur(2px)!important;
	filter: blur(2px);
	-moz-filter: blur(2px);
	-o-filter: blur(2px);
	-ms-filter: blur(2px);
	filter:progid:DXImageTransform.Microsoft.Blur(PixelRadius='2');
	filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale"); /* Firefox 10+, Firefox on Android */
    filter: gray;
	transition: all 0.5s ease;
	-webkit-transform: translate3d(0, 0, 0);
}

